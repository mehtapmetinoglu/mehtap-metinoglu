<script>
        var googleAdsLoading = document.getElementById('googleAdsLoading');
        var process = document.getElementById('topBannerWithAds');
        var googleAddContainer = document.getElementById('googleAddContainer');

        if(!window['canRunAds']) {
            if(googleAdsLoading) {
               googleAdsLoading.classList.add('hidden');
            }

            if(process) {
               process.classList.remove('process');
            }
        }

        setTimeout(function() {
          if(!window['adsViewable']) {
             if(googleAdsLoading) {
                googleAdsLoading.classList.add('hidden');
              }

             if(googleAddContainer) {
                googleAddContainer.classList.add('hidden');
             }

             if(process) {
               process.classList.remove('process');
             }
          }
        }, 10000)
</script>

